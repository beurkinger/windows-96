{
  "name": "preact-starter-kit",
  "version": "1.0.0",
  "description": "Some boilerplate code to kickstart Preact-based projects.",
  "main": "index.html",
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "gh-pages -d dist",
    "lint": "eslint . --ext .ts,.tsx",
    "start": "webpack-dev-server --mode development --open",
    "type-check": "tsc --noEmit --watch",
    "update-config": "node ./scripts/update-config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beurkinger/preact-starter-kit.git"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "javascript",
    "typescript",
    "webpack",
    "eslint",
    "prettier",
    "post-css",
    "html5",
    "css3",
    "framework"
  ],
  "author": "Thibault Goehringer",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/beurkinger/preact-starter-kit/issues"
  },
  "homepage": "https://github.com/beurkinger/preact-starter-kit#readme",
  "dependencies": {
    "preact": "^10.4.4"
  },
  "devDependencies": {
    "@teamsupercell/typings-for-css-modules-loader": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "file-loader": "^4.3.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
